<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		id="equipDialog"
		title="Equipment Mismatch" type="Message" afterClose="destroyMe">
		<content>
			<VBox>
			<Label design="Bold" id="eq1" text="Equipment Dispatched: {Equipment}"/>
			<Label id="eq2" text="Equipment 1 Description"/>
			<Label design="Bold" id="eq3" text="Equipment Scanned: {detailView>/barCodeScanned}" class="sapUiTinyMarginTop"/>
			<Label id="eq4" text="Equipment 2 Description" />
			<Label design="Bold" text="Equipment Mismatch" class="myRed sapUiTinyMarginTop"/>
			<Label design="Bold" text="Update with scanned ID?" class="sapUiSmallMarginTop"/>
			</VBox>
		</content>
		<beginButton>
			<Button text="Yes" press="equipmentMismatchSubmit"/>
		</beginButton>
		<endButton>
			<Button text="No" press="oDialogClose"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>